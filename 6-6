#include <stdio.h>

int reverse( int number );

int main()
{
    int n;

    scanf("%d", &n);
    printf("%d\n", reverse(n));

    return 0;
}

int reverse( int number ){
    int sum = 0;
    int first = 0;
    if(number < 0) {
        printf("-");
        number = -number;
    }
    
    while(number){
        if(!first){
            if(number % 10){
                first = !first;
                sum = sum * 10 + number % 10;
            }
        }
        else {
            sum = sum * 10 + number % 10;
        }
        
        number /= 10;
    }
    return sum;
}
